<?xml version="1.0"?>
<!--
Web.config file for TestWebSite.

The settings that can be used in this file are documented at 
http://www.mono-project.com/Config_system.web and 
http://msdn2.microsoft.com/en-us/library/b5ysx397.aspx
-->
<configuration>
  <system.web>
    <compilation defaultLanguage="C#" debug="true" targetFramework="4.0">
      <assemblies/>
    </compilation>
    <customErrors mode="RemoteOnly"/>
    <authentication mode="None"/>
    <authorization>
      <allow users="*"/>
    </authorization>
	<httpModules>
		<remove name="Session" />
		<add name="Session" type="AngiesList.Redis.RedisSessionStateModule" />
	</httpModules>
    <httpHandlers/>
    <trace enabled="true" localOnly="true" pageOutput="false" requestLimit="10" traceMode="SortByTime"/>
    <sessionState stateConnectionString="tcp=devcache01:6379" mode="Custom" customProvider="RedisSession" cookieless="false" timeout="10">
      <providers> 
        <add name="RedisSession" type="AngiesList.Redis.RedisSessionStateStore, AngiesList.Redis"/>
      </providers>
    </sessionState>
    <globalization requestEncoding="utf-8" responseEncoding="utf-8"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
	<system.webServer>
		<modules>
			<remove name="Session" />
			<add name="Session" type="AngiesList.Redis.RedisSessionStateModule" />
		</modules>
	</system.webServer>
</configuration>
